{
  "AWSEBDockerrunVersion": 2,
  "Authentication": {
    "Bucket": "elasticbeanstalk-us-east-1-229683044909",
    "Key": ".dockercfg"
  },
  "containerDefinitions": [
    {
      "name": "nit-ux-qa",
      "image": "kaptest/nursing:qa",
      "essential": true,
      "memory": 512,
      "mountPoints": [
        {
          "containerPath": "/var/log/nginx", 
          "sourceVolume": "awseb-logs-nit-ux-qa"
        }
      ],
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 80
        }
      ]
    },
    {
      "name": "nit-ux-dev",
      "image": "kaptest/nursing:dev",
      "essential": true,
      "memory": 128,
      "mountPoints": [
        {
          "containerPath": "/var/log/nginx", 
          "sourceVolume": "awseb-logs-nit-ux-qa"
        }
      ],
      "portMappings": [
        {
          "hostPort": 9000,
          "containerPort": 80
        }
      ]
    }
  ]
}